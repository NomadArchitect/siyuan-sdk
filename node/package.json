{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@siyuan-community/siyuan-sdk",
    "version": "0.0.1",
    "description": "A simple and easy to use SDK for SiYuan.",
    "keywords": [
        "api",
        "sdk",
        "siyuan",
        "siyuan-note",
        "siyuan-community"
    ],
    "homepage": "https://github.com/siyuan-community/siyuan-sdk/node",
    "bugs": {
        "url": "https://github.com/siyuan-community/siyuan-sdk/issues"
    },
    "license": "AGPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "https://github.com/siyuan-community/siyuan-sdk.git",
        "directory": "node"
    },
    "author": {
        "name": "Zuoqiu Yingyi",
        "url": "https://github.com/Zuoqiu-Yingyi"
    },
    "contributors": [
        {
            "name": "SiYuan Community",
            "email": "zuoez02@siyuan-note.club",
            "url": "https://github.com/siyuan-community"
        }
    ],
    "funding": [
        {
            "type": "AFDIAN",
            "url": "https://afdian.net/a/zuoqiu"
        },
        {
            "type": "Ko-fi",
            "url": "https://ko-fi.com/zuoqiu"
        }
    ],
    "main": "./dist/index.umd.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "browser": "./dist/index.iife.js",
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "files": [
        "dist",
        "package.json",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "prebuild": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./scripts/prebuild.ts",
        "build": "vite build",
        "format": "eslint --fix .",
        "lint": "eslint . && cspell .",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "devDependencies": {
        "@ltd/j-toml": "^1.38.0",
        "@nodelib/fs.walk": "^1.2.8",
        "@types/node": "^20.3.1",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "@vitest/coverage-v8": "^0.32.2",
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1",
        "cspell": "^6.31.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.43.0",
        "json-schema-to-typescript": "^13.0.2",
        "json5": "^2.2.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3",
        "vite": "^4.3.9",
        "vite-plugin-dts": "^2.3.0",
        "vite-plugin-static-copy": "^0.16.0",
        "vitest": "^0.32.2"
    }
}
