{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@siyuan-community/siyuan-sdk",
    "version": "0.1.3",
    "description": "A simple and easy to use SDK for SiYuan.",
    "keywords": [
        "api",
        "sdk",
        "siyuan",
        "siyuan-note",
        "siyuan-community"
    ],
    "homepage": "https://github.com/siyuan-community/siyuan-sdk/tree/main/node",
    "bugs": {
        "url": "https://github.com/siyuan-community/siyuan-sdk/issues"
    },
    "license": "AGPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "https://github.com/siyuan-community/siyuan-sdk.git",
        "directory": "node"
    },
    "author": {
        "name": "Zuoqiu Yingyi",
        "url": "https://github.com/Zuoqiu-Yingyi"
    },
    "contributors": [
        {
            "name": "SiYuan Community",
            "email": "zuoez02@siyuan-note.club",
            "url": "https://github.com/siyuan-community"
        }
    ],
    "funding": [
        {
            "type": "Ko-fi",
            "url": "https://ko-fi.com/zuoqiu"
        },
        {
            "type": "AFDIAN",
            "url": "https://afdian.net/a/zuoqiu"
        }
    ],
    "main": "./dist/index.umd.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "browser": "./dist/index.iife.js",
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "files": [
        "dist",
        "package.json",
        "README.md",
        "README-zh-Hans.md",
        "LICENSE"
    ],
    "scripts": {
        "dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./scripts/dev.ts",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:once": "vitest run",
        "prebuild": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./scripts/prebuild.ts",
        "build": "vite build",
        "publish:npm": "npm run prebuild && npm run build && npm publish",
        "format": "eslint --fix .",
        "lint": "eslint . && cspell .",
        "coverage": "vitest run --coverage",
        "check": "npm run lint && npm run format && npm run coverage"
    },
    "devDependencies": {
        "@ltd/j-toml": "^1.38.0",
        "@nodelib/fs.walk": "^1.2.8",
        "@types/node": "^20.4.5",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "@vitest/coverage-istanbul": "^0.33.0",
        "@vitest/ui": "^0.33.0",
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1",
        "chokidar": "^3.5.3",
        "cspell": "^6.31.2",
        "dotenv": "^16.3.1",
        "eslint": "^8.45.0",
        "json5": "^2.2.3",
        "quicktype-core": "^23.0.63",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "vite": "^4.4.7",
        "vite-plugin-dts": "^3.3.1",
        "vite-plugin-static-copy": "^0.16.0",
        "vitest": "^0.32.4"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "ofetch": "^1.1.1"
    }
}
