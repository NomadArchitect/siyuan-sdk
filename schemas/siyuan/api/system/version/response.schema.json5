/*
 * schemas/siyuan/api/system/version/response.schema.json5
 * 获取思源内核版本号
 */
{
    $schema: "https: //json-schema.org/draft/2020-12/schema",
    $id: "https://raw.githubusercontent.com/siyuan-community/siyuan-sdk/main/schemas/siyuan/api/system/version/response.schema.json5",
    title: "I version response",
    description: "Get the version of siyuan kernel",
    type: "object",
    properties: {
        code: {
            type: "integer",
            description: "status code",
        },
        msg: {
            type: "string",
            description: "status message",
        },
        data: {
            type: "string",
            description: "semantic version number of siyuan kernel",
        },
    },
    required: [
        "code",
        "msg",
        "data",
    ],
}
