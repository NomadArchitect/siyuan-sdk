{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://raw.githubusercontent.com/siyuan-community/siyuan-sdk/main/schemas/kernel/api/notification/pushMsg/response.schema.json",
    "$comment": "v2.9.2",
    "$ref": "#/definitions/root",
    "definitions": {
        "root": {
            "title": "response body",
            "description": "push alert information",
            "type": "object",
            "additionalProperties": false,
            "required": [
                "code",
                "msg",
                "data"
            ],
            "properties": {
                "code": {
                    "type": "integer",
                    "description": "status code"
                },
                "msg": {
                    "type": "string",
                    "description": "status message"
                },
                "data": {
                    "$ref": "#/definitions/data"
                }
            }
        },
        "data": {
            "title": "IData",
            "description": "the related info of the notification",
            "type": "object",
            "additionalProperties": false,
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "type": "string",
                    "description": "message ID",
                    "pattern": "^[0-9a-z]{7}$"
                }
            }
        }
    }
}