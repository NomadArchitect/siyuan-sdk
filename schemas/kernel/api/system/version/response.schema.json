{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://raw.githubusercontent.com/siyuan-community/siyuan-sdk/main/schemas/kernel/api/system/version/response.schema.json",
    "$comment": "v2.9.2",
    "type": "object",
    "title": "I version response",
    "description": "Get the version of siyuan kernel",
    "additionalProperties": false,
    "properties": {
        "code": {
            "type": "integer",
            "description": "status code"
        },
        "msg": {
            "type": "string",
            "description": "status message"
        },
        "data": {
            "type": "string",
            "description": "semantic version number of siyuan kernel",
            "pattern": "^(\\d+)\\.(\\d+)\\.(\\d+)$"
        }
    },
    "required": [
        "code",
        "msg",
        "data"
    ]
}