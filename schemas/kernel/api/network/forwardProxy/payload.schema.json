{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://github.com/siyuan-community/siyuan-sdk/raw/main/schemas/kernel/api/network/forwardProxy/payload.schema.json",
    "$comment": "v2.9.5",
    "$ref": "#/$defs/root",
    "$defs": {
        "root": {
            "title": "payload body",
            "description": "Forward proxy",
            "type": "object",
            "additionalProperties": false,
            "required": [
                "url",
                "headers"
            ],
            "properties": {
                "url": {
                    "type": "string",
                    "description": "URL to request",
                    "format": "uri"
                },
                "method": {
                    "type": "string",
                    "description": "HTTP method to request\n@default: \"GET\"",
                    "enum": [
                        "GET",
                        "POST",
                        "PUT",
                        "PATCH",
                        "DELETE",
                        "HEAD",
                        "OPTIONS",
                        "CONNECT"
                    ],
                    "default": "GET"
                },
                "timeout": {
                    "type": "integer",
                    "description": "timeout to request (ms)\n@default: 7000",
                    "minimum": 0,
                    "default": 7000
                },
                "contentType": {
                    "type": "string",
                    "description": "Content-Type for request body\n@default: \"application/json\"",
                    "default": "application/json"
                },
                "headers": {
                    "type": "array",
                    "description": "request headers list",
                    "items": {
                        "$ref": "#/$defs/headers"
                    }
                },
                "payload": {
                    "type": [
                        "object",
                        "string"
                    ],
                    "description": "request body"
                }
            }
        },
        "headers": {
            "title": "IHeaders",
            "description": "Request headers",
            "type": "object",
            "additionalProperties": {
                "type": "string"
            }
        }
    }
}